{
  "expo": {
    "name": "minhaVenda",
    "slug": "minhaVenda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_home.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo_light.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.expo.minhavenda"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_light.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.expo.minhavenda",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "43f16a23-bdfb-45d7-b56a-49829d136765"
      }
    },
    "owner": "acaijaborandi"
  }
}
